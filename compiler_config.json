{
  "language": "Solidity",
  "settings": {
    "optimizer": {
      "enabled": true,
      "runs": 8000,
      "details": {
        "constantOptimizer": true  
      }
    },
    "viaIR": true,
    "evmVersion": "prague",

    "outputSelection": {
      "*": {
        "":   [ "ast" ],
        "*": [
          "abi",
          "metadata",
          "devdoc",
          "userdoc",
          "storageLayout",
          "evm.legacyAssembly",
          "evm.bytecode",
          "evm.deployedBytecode",
          "evm.methodIdentifiers",
          "evm.gasEstimates",
          "evm.assembly"
        ]
      }
    }
  }
}
